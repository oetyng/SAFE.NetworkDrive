<Window x:Class="SAFE.NetworkDrive.UI.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SAFE.NetworkDrive.UI"
        mc:Ignorable="d"
        Title="Login" Height="121.553" Width="286.496" Icon="../Resources/favicon_white.ico">
    <Grid Margin="0,0,2,0">
        <PasswordBox Name="pwdBoxPassword" KeyUp="PwdBoxPassword_KeyUp" HorizontalAlignment="Left" 
                 Height="23" Margin="100,19,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Name="BtnLogin" Content="OK" HorizontalAlignment="Left" Margin="100,47,0,0" 
                VerticalAlignment="Top" Width="120" Click="BtnLogin_Click"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="22,19,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.339,0.577"/>
    </Grid>
</Window>